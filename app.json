{
  "expo": {
    "name": "SaathiCircle",
    "slug": "saathicircle",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "saathicircle",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#5B9BD5"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.saathicircle.app",
      "icon": "./assets/icon.png"
    },
    "android": {
      "package": "com.saathicircle.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#5B9BD5"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.VIBRATE",
        "android.permission.CALL_PHONE"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "d4162acb-32a7-42f3-9944-2700cbf7908e"
      }
    }
  }
}
